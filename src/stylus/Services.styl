.box {
    width: 1920px;

    .Services-boxx {
        width: 100%;
        height: 1000px;
        background: #F6F6F6;
        padding-top: 40px;

        .Services-model {
            width: 70%;
            height: 400px;
            display: flex;
            margin: auto;

            .Services-left {
                width: 30%;
                height: 400px;
                background: #2D6A4F;

                div {
                    width: 100%;
                    height: 16.7%;
                    line-height: 66.8px;
                    text-align: center;
                    color: #fff;
                    border-bottom: 1px solid #979797;
                }
            }

            .Services-right {
                width: 80%;
                height: 16.7%;
                height: 400px;
                background: #fff;

                .right-box {
                    width: 100%;
                    height: 16.7%;
                    padding: 10px 10px;
                    display: flex;
                    line-height: 40px;
                    border-bottom: 1px solid #979797;

                    div {
                        margin-right: 15px;
                    }
                }

                .right-input {
                    input {
                        margin: 0px 10px;
                    }

                    button {
                        width: 150px;
                        background: #73C153;
                        color: #fff;
                    }
                }
            }
        }

        .shaed-box {
            width: 70%;
            height: 400px;
            background: red;
            margin: 40px auto 0px;
        }

        .shaed-box-left {
            width: 70%;
            height: 100px;
            background: pink;

            .shaed-box-ul {
                display: flex;
                flex-wrap: wrap;

                li {
                    width 125px
                    background: #ff9900;
                    margin-right: 20px;
                }
            }
        }
    }
}